{
    "name": "mjohnson/utility",
    "type": "cakephp-plugin",
    "description": "A collection of CakePHP utility libraries.",
    "keywords": [
        "cakephp", "utility", "plugin", "controller", "component", "model", "behavior", "datasource", "shell", "helper",
        "auto-login", "ajax-handler", "cacheable", "convertable", "enumerable", "spam-blocker", "validateable", "cache-kill",
        "feed", "sluggable", "aggregator", "filterable", "open-graph", "sitemap", "bbcode", "decoda", "markup"
    ],
    "homepage": "http://milesj.me/code/cakephp/utility",
    "license": "MIT",
    "authors": [
        {
            "name": "Miles Johnson",
            "homepage": "http://milesj.me"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-mbstring": "*",
        "composer/installers": "*"
    },
    "suggest": {
        "mjohnson/decoda": "Enable Decoda for BBCode parsing in the DecodaHelper",
        "titon/utility": "Enable Titon to parse feeds in FeedSource and process AJAX in the AjaxHandlerComponent"
